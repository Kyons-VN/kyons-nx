{
  "$schema": "./node_modules/nx/schemas/workspace-schema.json",
  "version": 2,
  "projects": {
    "school": "apps/school",
    "school-e2e": "apps/school-e2e",
    "share-components": "libs/share-components",
    "share-directives": "libs/share-directives",
    "share-pipes": "libs/share-pipes",
    "share-utils": "libs/share-utils",
    "student": "apps/student",
    "student-e2e": "apps/student-e2e",
    "theme": "libs/theme",
    "waiting-list": "apps/waiting-list",
    "waiting-list-e2e": "apps/waiting-list-e2e"
  },
  "build": {
    "builder": "@angular-devkit/build-angular:browser"
  },
  "defaultProject": "student",
  "architecture": {
    "server": {
      "builder": "@angular-devkit/build-angular:browser",
      "options": {
        "browserTarget": "student:serve:dev",
        "proxyConfig": "apps/student/src/proxy.conf.json"
      }
    }
  }
}

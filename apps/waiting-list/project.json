{
  "name": "waiting-list",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/waiting-list/src",
  "prefix": "wl",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/waiting-list/prod",
        "index": "apps/waiting-list/src/index.html",
        "main": "apps/waiting-list/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/waiting-list/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/waiting-list/src/favicon.ico",
          "apps/waiting-list/src/assets",
          {
            "glob": "**/*",
            "input": "libs/theme/src/assets/",
            "output": "/assets/"
          }
        ],
        "styles": ["libs/theme/src/styles.scss", "apps/waiting-list/src/styles.scss"],
        "scripts": [],
        "allowedCommonJsDependencies": ["lottie-web"]
      },
      "configurations": {
        "prod": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/waiting-list/src/environments/environment.ts",
              "with": "apps/waiting-list/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "dev": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "prod"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "prod": {
          "browserTarget": "waiting-list:build:prod"
        },
        "dev": {
          "browserTarget": "waiting-list:build:dev"
        }
      },
      "defaultConfiguration": "dev"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "waiting-list:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/waiting-list/**/*.ts", "apps/waiting-list/**/*.html"]
      }
    }
  },
  "tags": []
}
